<mvc:View
    controllerName="com.yash.demojd3northwind.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
                        <List id="productList" items="{ path:'/Employees' }">
							<items>
								<ObjectListItem title="{FirstName}"
									type="Navigation" press="onListItemPress"></ObjectListItem>
							</items>
						</List>
                    </content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
